<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shuntai.mapper.TokenMapper">




    <select id="getToken" resultType="com.foxinmy.weixin4j.model.Token">
        select accessToken,createTime from token where id=1;
    </select>




    <update id="updateToken" parameterType="com.foxinmy.weixin4j.model.Token">
        update token SET accessToken=#{accessToken},createTime=#{createTime}
        where id=1
    </update>
</mapper>
